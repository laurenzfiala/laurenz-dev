{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "schematics": {
    "@schematics/angular:component": {
      "style": "css",
      "skipTests": true,
      "changeDetection": "OnPush",
      "path": "src/app/components"
    },
    "@schematics/angular:class": {
      "skipTests": true,
      "path": "src/app/classes"
    },
    "@schematics/angular:directive": {
      "skipTests": true,
      "path": "src/app/directives"
    },
    "@schematics/angular:guard": {
      "skipTests": true,
      "functional": true,
      "path": "src/app/guards"
    },
    "@schematics/angular:module": {
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "skipTests": true,
      "path": "src/app/pipes"
    },
    "@schematics/angular:service": {
      "skipTests": true,
      "path": "src/app/services"
    },
    "@schematics/angular:interface": {
      "path": "src/app/interfaces"
    },
    "@schematics/angular:interceptor": {
      "path": "src/app/interceptors"
    }
  },
  "projects": {
    "laurenz-dev-reboot-new": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/laurenz-dev-reboot-new",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets", "src/.htaccess"],
            "styles": ["src/styles/layers.css", "src/styles/index.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "subresourceIntegrity": true,
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "laurenz-dev-reboot-new:build:production"
            },
            "development": {
              "browserTarget": "laurenz-dev-reboot-new:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "laurenz-dev-reboot-new:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles/index.css"],
            "scripts": []
          }
        }
      }
    }
  }
}
